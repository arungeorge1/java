import java.util.Scanner;
public class sample{
    public static void main(String[] arg){
     Scanner sc=new Scanner(System.in);
     System.out.println("Enter the string: ");
     String str=sc.nextLine();
     String s="";
     s=s+str.charAt(0);
     for(int i=0;i<str.length()-1;i++){
         if(str.charAt(i)==' '){
             s=s+str.charAt(i+1);
         }
     }
     System.out.println(s);   
    }
    
}
